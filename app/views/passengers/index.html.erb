<h2>Passenger List</h2>
<h4>There are currently <%= @passengers.length %> registered passengers.</h4>
<main>
  <ul>
    <% @passengers.each do |passenger| %>
      <li>
        <p>ID: <strong><%=link_to passenger.id, passenger_path(passenger.id)%></strong> Name: <span class="fas fa-paw"></span>  <strong><%=link_to passenger.name, passenger_path(passenger.id)%></strong> Phone Number: <%=passenger.phone_num%></p>
      </li>
    <% end %>
  </ul>
</main>
